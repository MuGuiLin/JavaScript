body,img {
	border: 0px;
	padding: 0px;
	margin: 0px;
}
body,html{height:100%;width: 100%;overflow: hidden;}
body{
	background: url(img/tema.jpg)  no-repeat;
	background-size: 100% 100%;
}
.map {
	width: 100%;
	position: absolute;
	top: 40%;
}

.extract img{
	display: block;
	width: 100px;
	height:100px;
	border-radius: 50%;
	margin: 20px auto;
}

.importaa {
	max-width: 400px;
	display: block;
	width: 40%;
	height: 40px;
	border: 1px solid #ffff00;
	border-radius: 50px;
	text-align: center;
	font-size: 30px;
	font-weight: bold;
	color: #FFFF00;
	margin: 0 auto;
	font-family: Arial, Helvetica, sans-serif;
	background: rgba(0,0,0,0.5);
	
}
.return_home{
	width: auto;
	float:left;
	margin:112px 0 0 43%;
}
.return_home a{
	font-size:12px;
	color:#1d6cb3;
	line-height:22px;
	text-decoration:none;
}
.top img{
	width: 100%;
	height: 100%;
	border: none;
}
.top{
	width: 100%;
	height: 40%;
	border: none;
}
.bottom img{
	width: 100%;
	height: 100%;
}
.bottom{
	position: absolute;
	bottom: 1%;
	width: 100%;
	height: 20%;
	z-index: -1;
}
.text{
	margin-top: 5%;
	text-align: center;
}
.text a{
	color:red ;
	font-weight: bold;
	font-size: 14px;
	text-decoration: none;
	}
.text span{
	position: relative;display: block;
	}
.text span:after{
	content: "";position: absolute;top:;left:;
	width: 20px;height: 20px;
	background-image: url(img/after.png);
	background-size: 100% 100%;
	}
	
	/* File Created: ʮ���� 28, 2011 */
.LayDashed {

}

.LayMain {
	position: absolute;
	margin: -110px 0px 0px -98px;
	position: absolute;
	top: 50%;
	left: 50%;
	font-family: Sans-Serif;
	font-size: 20px;
	display: none;
	z-index: 5;
	overflow: hidden;
	background-image: url(img/pop.png);
	background-size: 100% 100%;
}

.LayMainAll {

	-webkit-border-radius: 5px;
	-moz-border-radius: 5px;
	border-radius: 5px;
}


.LayMainTop {
	height: 26px;
	cursor: default;
	font-size: 20px;
	padding: 5px;
	font-weight: bold;
	color: #FF0;
	text-align: center;
	margin: 30px 2px 5px 5px;
}

.LayMainMiddle {
	height: 180px;
	color: #666666;
	font-weight: bold;
	
}

.LayMainFooter {
	height: 40px;
	text-align: right;
}

.LayShade {
	background-color: #000;
	position: absolute;
	display: none;
	left: 0px;
	top: 0px;
	width: 100%;
	z-index: 0;
	height: 100%;
	filter: Alpha(Opacity = 30);
	/* IE */
	-moz-opacity: 0.4;
	/* Moz + FF */
	opacity: 0.4;
}

.LaySpan {
	position: absolute;
	float: right;
	margin: -20px 10px 0 20px;
	cursor: pointer;
	right: 2px;
}
.LaySpan img{
	width: 20px;
	height: 20px;
}

.MegInfo {
	width: 100%;
	margin-top: 20px;
	float: left;
	text-align: center;
	color:#E6E8FA;
	font-size: 36px;
}

.MegInfo img {
	vertical-align: middle;
	margin-right: 10px;
}

.LayBtn {
	width: 72px;
	height: 20px;
	margin-top: 10px;
	background-color: #F2F2F2;
	cursor: pointer;
	color: #666666;
	margin-left: 10px;
	border-radius: 10px;
}

.LayBtn:hover {
	color: #000;
}

#LayBtnOK {
	position: absolute;
	left: 52px;
}

#LayBtnCancel {
	position: absolute;
	left: 157px;
	
}